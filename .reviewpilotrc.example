{
    "baseBranch": "main",
    "excludePatterns": ["*.lock", "*.min.js", "dist/**"],
    "copilotTimeout": 30000,
    "outputDir": ".reviewpilot-output",
    "maxFileSizeKB": 500,
    "telemetry": true,
    "performanceBudgets": {
